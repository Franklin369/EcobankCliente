<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="EcobanPro.Vistas.Confi.ConfClientes">
    <ContentPage.Content>
        <StackLayout>
            <Entry Placeholder="Identificacion" x:Name="txtIdentificacion"/>
            <Entry Placeholder="Nombres apellidos" x:Name="txtNombresApell"/>

            <Button x:Name="btnguardar" Text="Guardar"
                    Clicked="btnguardar_Clicked"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>